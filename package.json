{
  "name": "js-sandbox",
  "version": "1.0.13",
  "description": "javascript sandbox parser",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json --fix",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "build": "tsc",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joehecn/js-sandbox.git"
  },
  "keywords": [
    "javascript",
    "sandbox",
    "parser"
  ],
  "author": "leanbrown@live.cn",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/joehecn/js-sandbox/issues"
  },
  "homepage": "https://github.com/joehecn/js-sandbox#readme",
  "files": [
    "lib/**/*"
  ],
  "devDependencies": {
    "@types/node": "^20.6.0",
    "@vitest/coverage-v8": "^0.34.4",
    "csvtojson": "^2.0.10",
    "fast-xml-parser": "^4.2.7",
    "prettier": "^3.0.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.4"
  },
  "peerDependencies": {
    "quickjs-emscripten": "^0.23.0"
  }
}
